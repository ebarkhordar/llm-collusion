    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug target_identification.py",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/target_identification.py",
            "console": "integratedTerminal",
            "args": [
                "--input",
                "${workspaceFolder}/data/code_generation/mbpp-sanitized/test",
                "--model1",
                "anthropic/claude-haiku-4.5",
