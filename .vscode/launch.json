{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: generate_pairs (mbpp 0..10)",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/generate_pairs.py",
      "args": [
        "--dataset",
        "mbpp",
        "--start-index",
        "0",
        "--end-index",
        "10"
      ],
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "justMyCode": true,
      "envFile": "${workspaceFolder}/.env"
    },
    {
      "name": "Python: cross_model_detection (mbpp test)",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/cross_model_detection.py",
      "args": [
        "run",
        "--dataset-folder",
        "mbpp-sanitized",
        "--split",
        "test",
        "--judge",
        "openai/gpt-5",
        "--target",
        "deepseek/deepseek-chat-v3-0324",
        "--other",
        "x-ai/grok-4-fast",
        "--temperature",
        "0.0"
      ],
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "justMyCode": true,
      "envFile": "${workspaceFolder}/.env",
      "env": {
        "PYTHONPATH": "${workspaceFolder}/src"
      }
    }
  ]
}


